                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        folding-cell:backSideColor="#ffffff">
                        ​

                        <LinearLayout
                            android:id="@+id/gsec_content"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:visibility="gone">

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:src="@drawable/razzmatazz" />

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:src="@drawable/razzmatazz" />

